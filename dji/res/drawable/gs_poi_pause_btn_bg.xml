<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <padding android:left="15dp" android:top="5dp" android:right="15dp" android:bottom="5dp" />
            <corners android:radius="2dp" />
            <stroke android:width="1dp" android:color="#991fa3f6" />
            <solid android:color="@color/eq" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <padding android:left="15dp" android:top="5dp" android:right="15dp" android:bottom="5dp" />
            <corners android:radius="2dp" />
            <stroke android:width="1dp" android:color="#991fa3f6" />
            <solid android:color="@color/eq" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <padding android:left="15dp" android:top="5dp" android:right="15dp" android:bottom="5dp" />
            <corners android:radius="2dp" />
            <stroke android:width="1dp" android:color="@color/h_" />
            <solid android:color="@color/eq" />
        </shape>
    </item>
</selector>
