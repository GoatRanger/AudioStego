<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="#4a4a4a" android:dashGap="0dp" />
            <solid android:color="@color/ly" />
            <corners android:radius="30dp" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="@color/ly" android:dashGap="0dp" />
            <solid android:color="#757575" />
            <corners android:radius="30dp" />
        </shape>
    </item>
</selector>
