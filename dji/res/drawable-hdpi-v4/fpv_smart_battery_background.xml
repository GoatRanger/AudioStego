<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/multiProgress">
        <clip android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:clipOrientation="horizontal">
            <shape android:shape="rectangle">
                <solid android:color="@color/i6" />
            </shape>
        </clip>
    </item>
    <item android:id="@+id/multiSecondaryProgress">
        <clip android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:clipOrientation="horizontal">
            <shape android:shape="rectangle">
                <solid android:color="@color/i8" />
            </shape>
        </clip>
    </item>
    <item android:id="@+id/multiThirdProgress">
        <clip android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:clipOrientation="horizontal">
            <shape android:shape="rectangle">
                <solid android:color="@color/i7" />
            </shape>
        </clip>
    </item>
</layer-list>
